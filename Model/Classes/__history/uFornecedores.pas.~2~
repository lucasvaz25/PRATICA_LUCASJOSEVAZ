unit uFornecedores;

interface

uses
  UPessoa;

type
  TTipoPessoa = ( TTpPFisica = 0, TTpPJuridica = 1 );

  TFornecedores = class( TPessoa )
  private
  protected
  public
    constructor Create;
    destructor Destroy;
    procedure Free;

    property TpPessoa: TTipoPessoa;

    procedure CopiarDados( Value: TFornecedores );
  end;

implementation

end.
